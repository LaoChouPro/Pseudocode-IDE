// 测试5: 数组
OUTPUT "Test 5: Arrays"

DECLARE numbers : ARRAY[1:5] OF INTEGER

numbers[1] <- 10
numbers[2] <- 20
numbers[3] <- 30
numbers[4] <- 40
numbers[5] <- 50

OUTPUT "Array elements:"
FOR i <- 1 TO 5
    OUTPUT "numbers[", i, "] =", numbers[i]
NEXT i

// 二维数组
DECLARE matrix : ARRAY[1:2, 1:3] OF INTEGER

matrix[1,1] <- 1
matrix[1,2] <- 2
matrix[1,3] <- 3
matrix[2,1] <- 4
matrix[2,2] <- 5
matrix[2,3] <- 6

OUTPUT "Matrix:"
FOR row <- 1 TO 2
    FOR col <- 1 TO 3
        OUTPUT matrix[row, col]
    NEXT col
NEXT row
