// 综合测试：计算平均值
OUTPUT "Comprehensive Test: Calculate Average"

DECLARE numbers : ARRAY[1:5] OF INTEGER
DECLARE total : INTEGER
DECLARE average : REAL

numbers[1] <- 10
numbers[2] <- 20
numbers[3] <- 30
numbers[4] <- 40
numbers[5] <- 50

total <- 0
FOR i <- 1 TO 5
    total <- total + numbers[i]
NEXT i

average <- total / 5

OUTPUT "Numbers:", numbers[1], numbers[2], numbers[3], numbers[4], numbers[5]
OUTPUT "Total:", total
OUTPUT "Average:", average
